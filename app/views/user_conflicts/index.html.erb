<p id="notice"><%= notice %></p>

<h1>User Conflicts</h1>

<table>
  <thead>
    <tr>
      <th>User access</th>
      <th>Conflict</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_conflicts.each do |user_conflict| %>
      <tr>
        <td><%= user_conflict.user_access_id %></td>
        <td><%= user_conflict.conflict_id %></td>
        <td><%= link_to 'Show', user_conflict %></td>
        <td><%= link_to 'Edit', edit_user_conflict_path(user_conflict) %></td>
        <td><%= link_to 'Destroy', user_conflict, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Conflict', new_user_conflict_path %>
